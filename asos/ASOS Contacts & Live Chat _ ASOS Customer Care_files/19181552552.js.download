ngle_experiment", "changes": null}, {"holdback": 0, "activation": {}, "integrationSettings": {}, "integrationStringVersion": 2, "viewIds": ["19455031733"], "experiments": [{"weightDistributions": null, "audienceName": "Standard Exclusions,Store COM,Store AU,Store US,Store ROW,Store ROE", "name": "EMF-323 - PDP - Was/Now price copy (MVT) (100% BBB)", "bucketingStrategy": null, "variations": [{"id": "20614932828", "actions": [{"viewId": "19455031733", "changes": [{"dependencies": [], "type": "custom_code", "id": "709290E2-4249-47C5-A40F-EFFC7577B6BA", "value": function($){var utils = window.optimizely.get("utils");

var NOW_TRANSLATION = "Now ";
var WAS_TRANSLATION = "Was ";
var FROM_TRANSLATION = "from ";

var rrpInnerText = "";
var prevPriceInnerText = "";
var currentInnerText = "";
var displayFrom;

function updatePrice() {
  // do nothing for Mix And Match
  var mixAndMatchElement = document.querySelectorAll('.mix-and-match')[0];

  if (mixAndMatchElement === undefined) {

    var rrpElement = document.querySelectorAll('.product-prev-price')[0];
    var prevPriceElement = document.querySelectorAll('.product-prev-price')[1];
    var currentPriceElement = document.querySelector('.current-price');
    var discountElement = document.querySelector('.product-discount-percent');
    var fromElement = document.querySelector('.price-from-discounted');

    if (rrpElement.innerText === "" && prevPriceElement.innerText === "") {
    	return;
    }

    if (displayFrom === undefined) {
    	displayFrom = fromElement.style.display === "";
    }

    if (rrpElement.innerText !== "") {
      rrpElement.style.textDecorationLine = "none";
      // rrpElement.innerText = rrpElement.innerText.replace("RRP", WAS_TRANSLATION);
      rrpElement.style.fontSize = "20px";
      rrpElement.style.textDecorationThickness = "1px"; // New
    } else if (prevPriceElement.innerText !== "" ) {
      prevPriceElement.style.textDecorationLine = "none";
      prevPriceElement.innerText = WAS_TRANSLATION + prevPriceElement.innerText;
      prevPriceElement.style.textDecorationThickness = "1px"; // New
      prevPriceElement.style.fontSize = "20px";
    }

    // Update the percentage discount
    if (discountElement !== undefined) {
    	discountElement.style.fontSize = "20px";
    }

    // Update current price 
    currentPriceElement.style.letterSpacing = "1px";

    // Update the From if present
	fromElement.style.fontSize = "20px";

	if (displayFrom) {
		fromElement.style.display = "none";
		currentPriceElement.innerText = FROM_TRANSLATION + currentPriceElement.innerText;
	} 

	currentPriceElement.innerText = NOW_TRANSLATION + currentPriceElement.innerText;

    rrpInnerText = rrpElement.innerText;
    prevPriceInnerText = prevPriceElement.innerText;
    currentInnerText = currentPriceElement.innerText;
  }
}

// No strikethrough
utils.waitForElement('.price-container').then(function() {
	updatePrice();
});

utils.waitForElement('#main-size-select-0').then(function() {
	var sizeSelectElement = document.getElementById("main-size-select-0");
	var rrpElement = document.querySelectorAll('.product-prev-price')[0];
    var prevPriceElement = document.querySelectorAll('.product-prev-price')[1];
    var currentPriceElement = document.querySelector('.current-price');
    var fromElement = document.querySelector('.price-from-discounted');

	sizeSelectElement.addEventListener("change", function() {
		if (
			(!rrpInnerText || rrpElement.innerText !== rrpInnerText) && 
			(!prevPriceInnerText || prevPriceElement.innerText !== prevPriceInnerText) && 
			currentPriceElement.innerText !== currentInnerText
		) {
			updatePrice();
		}
		if (displayFrom) {
			fromElement.style.display = "none";
		}
	});
});

utils.waitForElement('.colour-size-select').then(function() {
	var colourSelectElement = document.querySelector(".colour-size-select select");
	var rrpElement = document.querySelectorAll('.product-prev-price')[0];
    var prevPriceElement = document.querySelectorAll('.product-prev-price')[1];
    var currentPriceElement = document.querySelector('.current-price');
    var fromElement = document.querySelector('.price-from-discounted');

	colourSelectElement.addEventListener("change", function() {
		if (
			(!rrpInnerText || rrpElement.innerText !== rrpInnerText) && 
			(!prevPriceInnerText || prevPriceElement.innerText !== prevPriceInnerText) && 
			currentPriceElement.innerText !== currentInnerText
		) {
			updatePrice();
		}
		if (displayFrom) {
			fromElement.style.display = "none";
		}
	});
});
